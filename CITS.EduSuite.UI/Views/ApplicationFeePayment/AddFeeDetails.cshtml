@model CITS.EduSuite.Business.Models.ViewModels.ApplicationFeePaymentViewModel
@using CITS.EduSuite.Business.Models.ViewModels

@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/CitsValidation")
    @Scripts.Render("~/Content/DatePickerJS")
    @Scripts.Render("~/Content/Select2JS")
    @Scripts.Render("~/Content/JqueryRepeater")
    @Scripts.Render("~/Scripts/ApplicationFeePayment")
    @Scripts.Render("~/Scripts/FeeDetails")
    @Scripts.Render("~/Scripts/FeeFollowUp")
}

<header class="section-header">
    <div class="row px-1">
        <div class="col-12">
            <div class="tbl">
                <div class="tbl-row">
                    <div class="tbl-cell">
                        <h3>@EduSuiteUIResources.Add@EduSuiteUIResources.Hyphen@EduSuiteUIResources.Edit@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Fee</h3>
                        <ol class="breadcrumb breadcrumb-simple mb-2 float-sm-right">
                            <li><a href="#" onclick="location.href='@Url.Action("FeeDetailslist", "ApplicationFeePayment")' ">@EduSuiteUIResources.FeeDetails</a></li>
                            <li class="active">@EduSuiteUIResources.Add@EduSuiteUIResources.Hyphen@EduSuiteUIResources.Edit@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Fee</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>

    </div>

</header>
<section class="section-content mx-sm-3 mx-0">
    <div class="form-master">
        @Html.Action("ApplicationFeePayment", "ApplicationFeePayment", Model.ApplicationKey)
    </div>

</section>

