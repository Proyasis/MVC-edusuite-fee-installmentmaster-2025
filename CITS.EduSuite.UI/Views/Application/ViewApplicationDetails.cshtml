                                <thead>
                                    <tr>
                                        <th>@EduSuiteUIResources.AcademicTerm</th>
                                        <th>@EduSuiteUIResources.StudyMaterial@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Name</th>
                                        <th>@EduSuiteUIResources.StudyMaterial@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Code</th>
                                        <th>@EduSuiteUIResources.StudyMaterial@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Type</th>
                                        <th>@EduSuiteUIResources.Available</th>
                                        <th>@EduSuiteUIResources.Issued</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @*@foreach (var item in Model.StudyMaterials)
                                        {*@
                                    @for (var i = 0; i < Model.StudyMaterials.Count; i++)
                                    {
                                        <tr>
                                            @if (i == 0 || (i != 0 && Model.StudyMaterials[i].SubjectYear != Model.StudyMaterials[i - 1].SubjectYear))
