        <div id="cpApplication" class="accordion">
            <div class="card mb-1">
                <div class="card-header" data-toggle="collapse" href="#EducationDetails" data-parent="#cpApplication">
                    <i class="fa fa-graduation-cap"></i> @EduSuiteUIResources.EducationDetails

                </div>
                <div class="card-body collapse overflow-auto" id="EducationDetails" data-parent="#cpApplication">
                    @if (Model.StudentsCertificateReturnDetail.Count > 0)
                    {
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>@EduSuiteUIResources.SlNo</th>
                                    <th>@EduSuiteUIResources.Course</th>
                                    <th>@EduSuiteUIResources.University</th>
                                    <th>@EduSuiteUIResources.CompletedYear</th>
                                    <th>@EduSuiteUIResources.CertificateType</th>
                                    <th>@EduSuiteUIResources.Mark@EduSuiteUIResources.OpenBracket@EduSuiteUIResources.InPercentage@EduSuiteUIResources.CloseBracket</th>
                                    <th>@EduSuiteUIResources.FileType</th>
                                    <th>@EduSuiteUIResources.Current@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Status</th>
                                    <th>@EduSuiteUIResources.Status@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Flow</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (var i = 0; i < Model.StudentsCertificateReturnDetail.Count; i++)
                                {

                                    <tr>
                                        <td>@(i + 1)</td>
                                        <td>
                                            @Model.StudentsCertificateReturnDetail[i].EducationQualificationName
                                        </td>
                                        <td>
                                            @Model.StudentsCertificateReturnDetail[i].EducationQualificationUniversity
                                        </td>

                                        <td>
                                            @Model.StudentsCertificateReturnDetail[i].EducationQualificationYear
                                        </td>
                                        <td>
                                            @Model.StudentsCertificateReturnDetail[i].EducationQualificationCertificateType
                                        </td>
                                        <td>
                                            @Model.StudentsCertificateReturnDetail[i].EducationQualificationPercentage @EduSuiteUIResources.PercentageSymbol
                                        </td>
                                        <td>
                                            <div class="form-row">
                                                @if (Model.StudentsCertificateReturnDetail[i].EducationQualificationCertificatePath != null)
                                                {
                                                    <div class="col-12">
                                                        <fieldset class="form-group">
                                                            <div class="card" id="divCard">

                                                                <div class="col-sm-12 paddingNone text-center">
                                                                    <img class="img-responsive" src="@Url.Content(Model.StudentsCertificateReturnDetail[i].EducationQualificationCertificatePath)" alt=" " style="height: 85px; width:85px;margin: auto;margin-bottom: 3px;" />
                                                                </div>

                                                                <a class="btn btn-primary col-xs-12 card-title" target="_blank" style="position:relative" href="@Url.Content(Model.StudentsCertificateReturnDetail[i].EducationQualificationCertificatePath)" data-lightbox="example-set" data-title="@Model.StudentsCertificateReturnDetail[i].EducationQualificationName">@Model.StudentsCertificateReturnDetail[i].EducationQualificationName</a>
                                                            </div>
                                                        </fieldset>
                                                    </div>

                                                }
                                            </div>
                                        </td>
                                        <td>

                                            @Model.StudentsCertificateReturnDetail[i].CertificateStatusName
                                            <span>By</span>  @Model.StudentsCertificateReturnDetail[i].CertificateStatusBy
                                            <span>On</span> <span class="label label-danger"> @String.Format("{0:dd/MM/yyyy}", Model.StudentsCertificateReturnDetail[i].IssuedDate)</span>
                                        </td>

                                        <td>
                                            <ul class="timeline">
                                                @foreach (var item in Model.StudentsCertificateReturnDetail[i].ReturnCertificateDetails)
                                                {
                                                    <li>
                                                        <div>
                                                            <time datetime="@item.CertificatestatusDate">@item.CertificateStatusName</time>
                                                            <p> <span>By</span> @item.CertificateStatusBy</p>
                                                            <p> <span>On</span> @string.Format("{0:dd/MM/yyyy}", item.CertificatestatusDate)</p>
                                                        </div>
                                                    </li>
                                                }
                                            </ul>
                                        </td>
                                    </tr>
                                }

                            </tbody>
                        </table>

                    }
                </div>
            </div>
            <div class="card mb-1">
                <div class="card-header" data-toggle="collapse" href="#FeeDetails" data-parent="#cpApplication">
                    <i class="fa fa-money"></i>  @EduSuiteUIResources.FeeDetails

                </div>
                <div class="card-body collapse overflow-auto" id="FeeDetails" data-parent="#cpApplication">

                    <div class="col-12">
                        <section class="tabs-section">
                            <div class="tabs-section-nav">
                                <ul id="tab-feedetails" class="nav" role="tablist">
                                    <li class="nav-item">
                                        <a aria-controls="discover" role="tab" class="nav-link active" data-toggle="tab" href="#dvTotalFeeDetails">
                                            <span class="nav-link-in">
                                                @EduSuiteUIResources.FeeDetails
                                            </span>

                                        </a>
                                    </li>

                                    <li class="nav-item" id="dvInstallement">
                                        <a aria-controls="discover" role="tab" class="nav-link" data-toggle="tab" href="#dvInstallmentFeeDetails">
                                            <span class="nav-link-in">
                                                @EduSuiteUIResources.Installment@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Details
                                            </span>

                                        </a>
                                    </li>

                                </ul>
                            </div>
                            <div class="tab-content p-0">
                                <div class="tab-pane active" id="dvTotalFeeDetails"></div>
                                <div class="tab-pane fade" id="dvInstallmentFeeDetails">
                                </div>

                            </div>

                        </section>


                    </div>
                    @if (Model.FeePyamentDetails.Count > 0)
                    {

                        <table class="table table-bordered mt-4">
                            <thead class="thead-light">
                                <tr>
                                    <th>
                                        @EduSuiteUIResources.SlNo
                                    </th>
                                    <th>
                                        @EduSuiteUIResources.Receipt
                                    </th>
                                    <th>
                                        @EduSuiteUIResources.Date
                                    </th>
                                    <th>
                                        @EduSuiteUIResources.FeeType
                                    </th>
                                    <th>
                                        @EduSuiteUIResources.PaymentMode
                                    </th>
                                    <th>
                                        @EduSuiteUIResources.Paid
                                    </th>
                                    @*<th>
                                            @EduSuiteUIResources.Balance
                                        </th>*@
                                    <th>
                                        @EduSuiteUIResources.Remarks
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            @for (var i = 0; i < Model.FeePyamentDetails.Count; i++)
                            {
                                <tr>
                                    <td>
                                        @(i + 1)
                                    </td>
                                    <td>
                                        @Model.FeePyamentDetails[i].RecieptNo
                                    </td>
                                    <td>
                                        @string.Format("{0:dd/MM/yyyy}", Model.FeePyamentDetails[i].FeeDate)
                                    </td>
                                    <td>
                                        <ul class="list-unstyled">
                                            @foreach (var item in Model.FeePyamentDetails[i].ApplicationFeePaymentDetails)
                                            {

                                                <li class="col-md-9">
                                                    <em>@item.FeeTypeName</em>
                                                    @if (item.FeeYear != null)
                                                    {
                                                        <em>( @item.FeeYearText  )</em>
                                                    }
                                                </li>

                                            }
                                        </ul>
                                    </td>
                                    <td>
                                        <ul class="list-unstyled">
                                            <li>Paid By : <span class="text-red"> @Model.FeePyamentDetails[i].PaymentModeName</span></li>

                                            @if (Model.FeePyamentDetails[i].PaymentModeKey == DbConstants.PaymentMode.Cheque)
                                            {
                                                <li>@EduSuiteUIResources.ChequeOrDDNumber: <span> @Model.FeePyamentDetails[i].ChequeOrDDNumber</span></li>

                                                <li>@EduSuiteUIResources.ChequeOrDDDate: <span> @string.Format("{0:dd/MM/yyyy}", Model.FeePyamentDetails[i].ChequeClearanceDate)</span></li>
                                            }
                                            @*else if (Model.FeePyamentDetails[i].PaymentModeKey == DbConstants.PaymentModeSub.Card)
                                                {
                                                    <li>@EduSuiteUIResources.CardNumber: <span> @Model.FeePyamentDetails[i].CardNumber</span></li>
                                                }*@
                                            else if (Model.FeePyamentDetails[i].PaymentModeKey == DbConstants.PaymentMode.Bank)
                                            {
                                                <li>@EduSuiteUIResources.BankAccount: <span> @Model.FeePyamentDetails[i].BankAccountName</span></li>
                                            }
                                        </ul>
                                    </td>
                                    <td>
                                        @string.Format("{0:G29}", Model.FeePyamentDetails[i].ApplicationFeePaymentDetails.Select(AF => AF.FeeAmount).DefaultIfEmpty().Sum())
                                    </td>
                                    @*<td>
                                            @string.Format("{0:G29}", (Model.PersonalDetails.TotalFeeAmount - (Model.FeePyamentDetails[i].ApplicationFeePaymentDetails.Select(AF => AF.CashFlowTypeKey == DbConstants.CashFlowType.In ? AF.FeeAmount : -AF.FeeAmount).DefaultIfEmpty().Sum() + Model.FeePyamentDetails[i].BalanceFee)))

                                        </td>*@
                                    <td>
                                        @Model.FeePyamentDetails[i].FeeDescription
                                        <br />
                                        @if (Model.FeePyamentDetails[i].ChequeStatusKey != null)
                                        {
                                            if (Model.FeePyamentDetails[i].ChequeStatusKey == DbConstants.ProcessStatus.Approved)
                                            {
                                                <span style="color:green"> Cheque @Model.FeePyamentDetails[i].ChequeAction </span> <span> On @string.Format("{0:dd/MM/yyyy}", @Model.FeePyamentDetails[i].ChequeApprovedRejectedDate)</span>
                                            }
                                            else
                                            {
                                                <span style="color:red"> Cheque @Model.FeePyamentDetails[i].ChequeAction </span> <span> On @string.Format("{0:dd/MM/yyyy}", @Model.FeePyamentDetails[i].ChequeApprovedRejectedDate)</span>
                                            }
                                        }
                                    </td>
                                    <td>
                                        @if (Model.FeePyamentDetails[i].IsRefunded == true)
                                        {
                                            <span class="label label-danger" style="padding: 10px;">Refunded</span>
                                        }

                                    </td>
                                </tr>
                            }
                        </table>


                    }
                </div>
            </div>
            <div class="card mb-1">
                <div class="card-header" data-toggle="collapse" href="#DocumentDetails" data-parent="#cpApplication">
                    <i class="fa fa-file"></i> @EduSuiteUIResources.Documents@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Details

                </div>
                <div class="card-body collapse overflow-auto" id="DocumentDetails" data-parent="#cpApplication">
                    <div class="form-row">
                        @foreach (var item in Model.DocumentDetails)
                        {
                            if (item.DocumentTypeName != null)
                            {
                                <div class="col-lg-2">
                                    <fieldset class="form-group">
                                        <div class="card" id="divCard">
                                            @if (item.StudentDocumentName != null)
                                            {
                                                <div class="col-sm-12 paddingNone text-center">
                                                    <img class="img-responsive" src="@Url.Content(item.StudentDocumentPath)" alt=" " style="height: 125px; width: 102px;margin: auto;margin-bottom: 3px;" />
                                                </div>
                                                <a class="btn btn-primary col-xs-12 card-title" target="_blank" style="position:relative" href="@Url.Content(item.StudentDocumentPath)" data-lightbox="example-set" data-title="@item.DocumentTypeName">@item.DocumentTypeName</a>
                                            }
                                            else
                                            {
                                                <div class="col-sm-12 paddingNone text-center">
                                                    <img class="img-responsive" src="~/Images/Not_available.jpg" alt=" " style="height: 125px; width: 102px;margin: auto;margin-bottom: 3px;" />
                                                </div>
                                                <a class="btn btn-primary col-xs-12 card-title" style="position:relative" data-lightbox="example-set" data-title="@item.DocumentTypeName">@item.DocumentTypeName</a>

                                            }
                                        </div>
                                    </fieldset>
                                </div>

                            }
                        }
                    </div>


                </div>
            </div>
            @if ((bool)ViewBag.ShowStudyMaterialt)
            {
                <div class="card mb-1">
                    <div class="card-header" data-toggle="collapse" href="#StudyMaterial" data-parent="#cpApplication">
                        <i class="fa fa-book"></i> @EduSuiteUIResources.StudyMaterial@EduSuiteUIResources.BlankSpace@EduSuiteUIResources.Details

                    </div>
                    <div class="card-body collapse overflow-auto" id="StudyMaterial" data-parent="#cpApplication">
