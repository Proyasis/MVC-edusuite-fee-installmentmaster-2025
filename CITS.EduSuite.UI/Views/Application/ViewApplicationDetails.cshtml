@using CITS.EduSuite.UI

<input type="hidden" id="hdnAttendanceAcademicPerfomance" value="@Url.Action("AttendanceAcademicPerfomance", "Application", new { id = Model.RowKey })" />
<input type="hidden" id="hdnTotalFeeDetails" value="@Url.Action("TotalFeeDetails", "ApplicationFeePayment", new { id = Model.RowKey })" />
