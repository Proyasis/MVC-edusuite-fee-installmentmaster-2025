    $(document).ready(function () {

        //$("#spnApplicationStatus")[0].style.color = AppCommon.SetColorByBackgroundIntensity($("#spnApplicationStatus")[0].style.backgroundColor);
        setTimeout(function () {
            ApplicationFeePayment.BindTotalFeeDetails();
            ApplicationFeePayment.BindInstallmentFeeDetails();
            Application.GetAttendanceAcademicPerfomance();
        }, 500);


        var HasInstallment = '@Model.PersonalDetails.HasInstallment';
            HasInstallment = HasInstallment ? JSON.parse(HasInstallment.toLowerCase()) : false;
            if (!HasInstallment) {
                $("#dvInstallement").hide();
            }
            else {
                $("#dvInstallement").show();
            }
        $("[khaleefa]").each(function (i) {


            var AppliedStatus = $("[id*=AppliedStatus]", $(this)).val();
            var html;
            if (AppliedStatus == "A") {
                html = "<span style=color:green>Applied</span>"
            }

        })

        $('[data-lightbox="example-set"]').each(function () {

            var ext = $(this).attr("href").split(".");
            ext = ext[ext.length - 1].toLowerCase();
            var fileExtension = ['pdf', 'doc', 'docx'];
            if (fileExtension.lastIndexOf((ext ? ext : "").toLowerCase()) > -1) {
                $(this).removeAttr("data-lightbox");
                var img = $(this).closest("#divCard").find("img");
                if (ext == "pdf") {
                    $(img).attr("src", 'data:image/png;base64,');
                }
                else {
                    $(img).attr("src", 'data:image/png;base64,');
                }
            }

        })

    })

    function GotoApplication(_this) {
        var applicationKey = $(_this)[0];
        applicationKey = parseInt(applicationKey) ? parseInt(applicationKey) : 0;


        href = $("#hdnViewApplication").val() + "/" + applicationKey
        window.location.href = href;
    }


</script>
<style>
    .timeline, .timeline li *::after, .timeline, .timeline li *::before {
        box-sizing: border-box;
        font-size: 9px;
    }
    /*.timeline ::after, .timeline ::before {
                box-sizing: border-box;
                font-size: 9px;
            }   .timeline li ::after, .timeline li::before {
                box-sizing: border-box;
                font-size: 9px;
            }*/

    .timeline {
        --timeline-color: #9bc;
        position: relative;
        list-style: none;
        display: inline-flex;
        flex-wrap: nowrap;
        margin: 0;
        padding: 0;
        /* set here the height of the timeline */
        height: 155px;
    }

        /* middle line */
        .timeline:before {
            content: "";
            position: absolute;
            top: calc(50% - 1px);
            width: 100%;
            height: 2px;
            background: var(--timeline-color);
        }

        .timeline li {
            margin: 0 0px;
            min-width: 100px;
            align-self: flex-start;
        }

            /*  event in even position are bottom-aligned */
            .timeline li:nth-child(2n) {
                align-self: flex-end;
            }

        .timeline div {
            position: relative;
            padding: 2px;
            border: 1px var(--timeline-color) solid;
        }

            .timeline div p {
                margin: 0px;
            }

                .timeline div p:last-child {
                    margin-bottom: 8px;
                }

        /* style for the dot over the timeline */
        .timeline li:before {
            content: "";
            position: absolute;
            top: 50%;
            margin-left: 50px;
            transform: translate(-50%, -50%);
            border: 1px #9bc solid;
            width: 10px;
            height: 10px;
            border-radius: 50%;
            background: var(--timeline-color);
        }

        /* style for the event arrow */
        .timeline div:before {
            content: "";
            position: absolute;
            left: 50%;
            top: 100%;
            width: 20px;
            height: 20px;
            transform: translate(-50%, -1px) rotateZ(45deg);
            background: #fff;
        }

        /* position of the event arrow in odd position */
        .timeline li:nth-child(2n - 1) div:before {
            top: 100%;
            margin-top: -8px;
            border-right: 1px var(--timeline-color) solid;
            border-bottom: 1px var(--timeline-color) solid;
        }

        /* position of the event arrow in even position */
        .timeline li:nth-child(2n) div:before {
            top: 0;
            margin-top: -10px;
            border-left: 1px var(--timeline-color) solid;
            border-top: 1px var(--timeline-color) solid;
        }
</style>