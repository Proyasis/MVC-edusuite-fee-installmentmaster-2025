                                    <a href=@Url.Action("DownloadFilePhoto", "Application", new { filename = Model.PersonalDetails.ApplicantPhoto,Studentname=Model.PersonalDetails.ApplicantName,AdmissionNo=Model.PersonalDetails.AdmissionNo})>
                                        <img class="img-thumbnail" src="@Url.Content(Model.PersonalDetails.ApplicantPhoto)" alt="User profile picture">
                                    </a>
                                }
                                else
